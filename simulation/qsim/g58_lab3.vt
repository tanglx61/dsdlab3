	data[0] = 1'b0;
end 

// rst
initial
begin
	rst = 1'b0;
	rst = #670000 1'b1;
	rst = #10000 1'b0;
end 

// clk
always
begin
	clk = 1'b0;
	clk = #2500 1'b1;
	#2500;

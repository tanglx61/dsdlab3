module g58_lab3_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [5:0] addr;
reg clk;
reg [5:0] data;
reg enable;
reg [1:0] mode;
reg rst;
// wires                                               
wire empty;
wire full;
wire [5:0] num;
wire [5:0] test;
wire [5:0] value;

wire sampler;                             

// assign statements (if any)                          
g58_lab3 i1 (
// port map - connection between master ports and signals/registers   
	.addr(addr),
	.clk(clk),
	.data(data),
	.empty(empty),
	.enable(enable),
	.full(full),
	.mode(mode),
	.num(num),
	.rst(rst),
	.test(test),
	.value(value)
);
// mode[ 1 ]
initial
begin
	mode[1] = 1'b1;
	mode[1] = #10000 1'b0;
end 
// mode[ 0 ]
initial
begin
	mode[0] = 1'b0;
end 
// addr[ 5 ]
initial
begin
	repeat(2)
	begin
		addr[5] = 1'b0;
		addr[5] = #320000 1'b1;
		# 320000;
	end
	addr[5] = 1'b0;
end 
// addr[ 4 ]
initial
begin
	repeat(4)
	begin
		addr[4] = 1'b0;
		addr[4] = #160000 1'b1;
		# 160000;
	end
	addr[4] = 1'b0;
end 
// addr[ 3 ]

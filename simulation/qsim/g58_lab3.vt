begin
	repeat(8)
	begin
		addr[3] = 1'b0;
		addr[3] = #80000 1'b1;
		# 80000;
	end
	addr[3] = 1'b0;
end 
// addr[ 2 ]
initial
begin
	repeat(16)
	begin
		addr[2] = 1'b0;
		addr[2] = #40000 1'b1;
		# 40000;
	end
	addr[2] = 1'b0;
end 
// addr[ 1 ]
initial
begin
	repeat(32)
	begin
		addr[1] = 1'b0;
		addr[1] = #20000 1'b1;
		# 20000;
	end
	addr[1] = 1'b0;
end 
// addr[ 0 ]
always
begin
	addr[0] = 1'b0;
	addr[0] = #10000 1'b1;
	#10000;
